<div class="card">
  <%= link_to (image_tag portfolio_img(portfolio_item.thumb_image,'thumb'), width:"100%"), portfolio_show_path(portfolio_item) %>
  <p class="card-text">
    <span><%= link_to portfolio_item.title, portfolio_show_path(portfolio_item) %></span>
    <%= link_to portfolio_item.subtitle  %>
  </p>
</div>
